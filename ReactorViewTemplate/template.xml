<template format="4"
        revision="1"
        name="ReactorView"
        description="Create a reactor view class and layout file">
 
    <category value="Other"/>
 
    <parameter id="className"
        name="Class Name"
        type="string"
        constraints="class|unique|nonempty"
        default="CustomReactorView"
        help="Class name for reactor view"/>
 
    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />
 
</template>